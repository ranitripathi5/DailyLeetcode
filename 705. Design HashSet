class MyHashSet {
    int [] set;
    public MyHashSet() {
        set=new int[1000000+1];
    }
    
    public void add(int key) {
        if(set[key]==0)
        set[key]=1;
    }
    
    public void remove(int key) {
        if(set[key]==1)set[key]=0;
    }
    
    public boolean contains(int key) {
        return(set[key]==1);
    }
}

